{
    "compilerOptions": {
        "target": "ESNext",
        "module": "commonJS",
        "moduleResolution": "Node",
        "resolveJsonModule": true,
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "outDir": "./build",
        "rootDirs": ["src", "prisma"]
    },
    "include": ["./src", "./prisma"]
}
